<Window x:Class="minecraft_server_gui.Admin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Админка" Height="300" Width="300" Icon="Chrisl21-Minecraft-3D-Grass.ico" ResizeMode="NoResize" WindowStartupLocation="CenterScreen">
    <Grid>
        <TextBox x:Name="Textbox_Admin_Input" HorizontalAlignment="Left" Height="23" Margin="10,11,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="228"/>
        <Button x:Name="Button_Admin_Enter" Content="GO" HorizontalAlignment="Left" Margin="243,11,0,0" VerticalAlignment="Top" Width="36" IsDefault="True" Click="Button_Admin_Enter_Click"/>
        <Button x:Name="Button_Admin_Close" Content="Закрыть" HorizontalAlignment="Left" Margin="204,235,0,0" VerticalAlignment="Top" Width="75" Click="Button_Admin_Close_Click"/>
        <Separator HorizontalAlignment="Left" Height="8" Margin="0,39,0,0" VerticalAlignment="Top" Width="292" Background="#FFB0B0B0"/>
        <Separator HorizontalAlignment="Left" Height="7" Margin="0,223,0,0" VerticalAlignment="Top" Width="292" Background="#FFB0B0B0"/>

    </Grid>
</Window>
